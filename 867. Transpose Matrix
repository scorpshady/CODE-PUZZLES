class Solution(object):
    def transpose(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: List[List[int]]
        """
        result = []
        for i in range(0 , len(matrix[0]) ):
            soln = []
            for j in range(0 , len(matrix)):
                
                soln.append(matrix[j][i])
            result.append(soln)
        
        return result
        
